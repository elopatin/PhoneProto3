<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="Alerts">
	
	<fx:Script>
		<![CDATA[
			protected function button1_clickHandler(event:MouseEvent):void
			{
				navigator.popToFirstView();
			}
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:navigationContent>
		<s:Button label="Back" click="button1_clickHandler(event)"/>
	</s:navigationContent>
	
	<s:Scroller width="100%" height="100%" top="100" horizontalCenter="0"
				horizontalScrollPolicy="off">
		<s:VGroup width="100%" height="100%" horizontalAlign="center" gap="10">
			<s:Group width="100%">
				<s:BitmapImage source="@Embed('assets/alertsFPO.png')" horizontalCenter="0"/>
			</s:Group>
		</s:VGroup>	
	</s:Scroller>
	
</s:View>
